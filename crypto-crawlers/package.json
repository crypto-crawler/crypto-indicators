{
  "name": "crypto-indicators",
  "version": "0.0.2",
  "description": "Calculate all kinds of indicators to assist cryptocurrency trading.",
  "bin": "dist/cli.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext '.js,.jsx,.ts,.tsx,.md'",
    "prettier": "prettier -c --write '**/*'",
    "prepare": "npm run build && npm run lint && npm run prettier"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepare"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulmachine/crypto-indicators.git"
  },
  "keywords": [
    "bitcoin",
    "blockchain",
    "cryptocurrency"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "soulmachine",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/soulmachine/crypto-indicators/issues"
  },
  "homepage": "https://github.com/soulmachine/crypto-indicators",
  "devDependencies": {
    "@types/lodash": "^4.14.161",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.6.4",
    "@types/redis": "^2.8.27",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "crypto-crawler": "^3.1.0",
    "crypto-markets": "^0.3.5",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "redis": "^3.0.2",
    "winston": "^3.3.3",
    "yargs": "^15.4.1"
  }
}
